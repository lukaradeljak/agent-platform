# ─── Onboarding Clients Agent ─────────────────────────────────────────────────
# Copiar a .env y completar los valores

# HubSpot CRM
HUBSPOT_ACCESS_TOKEN=

# Supabase (Auth + DB)
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# App redirect (optional — Supabase uses project default if not set)
APP_PASSWORD_RESET_REDIRECT_URL=

# Gmail (sending onboarding emails)
GMAIL_ADDRESS=
GMAIL_APP_PASSWORD=

# PayPal (optional — set ENABLE_PAYPAL=false to skip)
ENABLE_PAYPAL=false
PAYPAL_CLIENT_ID=
PAYPAL_SECRET=
PAYPAL_WEBHOOK_ID=
PAYPAL_MODE=sandbox
